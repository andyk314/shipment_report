<h1>New Report</h1>

<%= form_for @report, url: { action: 'create' } do |f| %>
  <%= f.date_field :start_date %>
  <%= label :start_date, 'Start Invoice Date' %><br />
  <%= f.date_field :end_date %>
  <%= label :end_date, 'End Invoice Date' %><br />
  <%= f.submit "Generate Report" %>
<% end %>

<%= link_to 'Back to Reports', reports_url %>
